			<xsd:element name="TotalEstateOrTrustLossAmt" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total estate or trust loss amount</Description>
						<LineNumber>36</LineNumber>
						<ELFFieldNumber>1937</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
