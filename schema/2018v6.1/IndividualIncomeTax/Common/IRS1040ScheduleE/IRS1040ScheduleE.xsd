				<xsd:annotation>
					<xsd:documentation>
						<Description>Total supplemental income or Loss</Description>
						<LineNumber>41</LineNumber>
						<ELFFieldNumber>2010</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
