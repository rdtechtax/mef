									<LineNumber>33(f)</LineNumber>
									<ELFFieldNumber>1825</ELFFieldNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<!-- End of EstateAndTrustGroup -->

			<!-- Estate and Trust Total Passive Income -->
			<xsd:element name="EstateAndTrustTotPssvIncmAmt" type="USAmountType" minOccurs="0">
