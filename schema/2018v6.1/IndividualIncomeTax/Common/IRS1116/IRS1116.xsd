						<xsd:element name="ForeignTaxWithheldOnIntAmt" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Foreign Tax Withheld on Interest Amount</Description>
									<LineNumber>o(A-C)</LineNumber>
									<ELFFieldNumber>0630 0730 0830</ELFFieldNumber>
