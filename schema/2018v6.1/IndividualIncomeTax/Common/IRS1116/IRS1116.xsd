					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="StringType">
						<xsd:enumeration value="AMT"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>

			<xsd:choice>
			        <!-- Foreign Income Section 951A Indicator -->
				<xsd:element name="ForeignIncmSection951AInd" type="CheckboxType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Foreign Income Section 951A Indicator</Description>
							<LineNumber>a</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<!-- Foreign Branch Income Indicator -->
				<xsd:element name="ForeignBranchIncomeInd" type="CheckboxType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Foreign Branch Income Indicator</Description>
							<LineNumber>b</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<!-- Foreign Income Passive Category Indicator -->
				<xsd:element name="ForeignIncPassiveCategoryInd" type="CheckboxType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Foreign Income Passive Category Indicator</Description>
							<LineNumber>c</LineNumber>
							<ELFFieldNumber>0020</ELFFieldNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<!-- Foreign Income General Category Indicator -->
				<xsd:element name="ForeignIncGeneralCategoryInd" type="CheckboxType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Foreign Income General Category Indicator</Description>
							<LineNumber>d</LineNumber>
							<ELFFieldNumber>0080</ELFFieldNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<!-- Foreign Income Section 901(j) Indicator -->
				<xsd:element name="ForeignIncSection901jInd" type="CheckboxType">
