									<LineNumber>3e(A-C)</LineNumber>
									<ELFFieldNumber>0250 0370 0490</ELFFieldNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<!-- Foreign Income Percentage -->
