								<xsd:documentation>
									<Description>Total Deduction Amount</Description>
									<LineNumber>3c(A-C)</LineNumber>
									<ELFFieldNumber>0230 0350 0470</ELFFieldNumber>
								</xsd:documentation>
							</xsd:annotation>
