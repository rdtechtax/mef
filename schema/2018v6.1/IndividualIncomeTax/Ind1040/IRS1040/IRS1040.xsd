								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<!-- Credit For Repayment Amount -->
						<xsd:element name="CreditForRepaymentAmt" type="USAmountType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Credit for repayment amount</Description>
									<LineNumber>Schedule 5, Line 74d</LineNumber>
									<ELFFieldNumber>1211</ELFFieldNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>

			<!-- Other Payments -->
			<xsd:element name="OtherPaymentsAmt" type="USAmountType" minOccurs="0">
