<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns="http://www.irs.gov/efile" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.irs.gov/efile" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">
	
	<xsd:annotation>
		<xsd:documentation>
			<Description>IRS e-file Schema - IRS Form 1040</Description>
			<TaxYear>2018</TaxYear>
			<MaturityLevel>Final Release</MaturityLevel>
			<ReleaseDate>February 13, 2019</ReleaseDate>
		</xsd:documentation>
	</xsd:annotation>

	<xsd:include schemaLocation="../../../Common/efileTypes.xsd"/>

	<!-- ================================================================== -->
	<!-- ======================= IRS Form 1040 ============================ -->
	<!-- ================================================================== -->

	<xsd:element name="IRS1040">
		<xsd:annotation>
			<xsd:documentation>IRS Form 1040</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="IRS1040Type">
					<xsd:attributeGroup ref="DocumentAttributes">
						<xsd:annotation>
							<xsd:documentation>Common return document attributes</xsd:documentation>
						</xsd:annotation>
					</xsd:attributeGroup>
					<xsd:attribute name="documentName" type="xsd:string" fixed="IRS1040">
						<xsd:annotation>
							<xsd:documentation>IRS internal use only. To avoid error in the return, do not include the attribute name or value.</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="referenceDocumentId" type="IdListType">
						<xsd:annotation>
							<xsd:documentation>List of document ID's of forms, schedules, supporting info. etc. attached to this form</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="referenceDocumentName" type="StringType" fixed="IRS965 IRS965A IRS8379 IRS8854 IRS8867 IRS8958 IRSRRB1042S IRSSSA1042S BackupWithholdingFromSchedulesK1Statement BinaryAttachment Chapter11BankruptcyStatement Elect1141110gFlowThruEntStatement Form1098DifferenceStatement NameChangeStatement OverseasExtensionStatement PenaltiesStatement SpecialDepreciationAllowanceElectionOutStatement SpouseSignatureStatement GeneralDependencyMedium GeneralDependencySmall">
						<xsd:annotation>
							<xsd:documentation>
								<ELFFieldNumber>1329</ELFFieldNumber>
							</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>

	<xsd:complexType name="IRS1040Type">
		<xsd:annotation>
			<xsd:documentation>Content model for IRS Form 1040</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
		
			<!-- Special Condition Description -->
			<xsd:element name="SpecialConditionDesc" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Special condition description</Description>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="TextType">
						<xsd:maxLength value="50"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			
			<!-- Primary Date of Death -->
			<xsd:element name="PrimaryDeathDt" type="DateType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Primary date of death</Description>
						<ELFFieldNumber>0020</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- Spouse Date Of Death -->
			<xsd:element name="SpouseDeathDt" type="DateType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Spouse date of death</Description>
						<ELFFieldNumber>0040</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<xsd:choice minOccurs="0">
				<!-- Special Processing Literal -->
				<xsd:element name="SpecialProcessingLiteralCd">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Special processing literal</Description>
							<ELFFieldNumber>0100</ELFFieldNumber>
						</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="TextType">
							<xsd:enumeration value="DESERT STORM"/>
							<xsd:enumeration value="HAITI"/>
							<xsd:enumeration value="FORMER YUGOSLAVIA"/>
							<xsd:enumeration value="UN OPERATION"/>
							<xsd:enumeration value="JOINT GUARD"/>
							<xsd:enumeration value="JOINT FORGE"/>
							<xsd:enumeration value="NORTHERN WATCH"/>
							<xsd:enumeration value="OPERATION ALLIED FORCE"/>
							<xsd:enumeration value="NORTHERN FORGE"/>
							<xsd:enumeration value="ENDURING FREEDOM"/>
							<xsd:enumeration value="IRAQI FREEDOM"/>
							<xsd:enumeration value="KOSOVO OPERATION"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="CombatZoneCd" fixed="COMBAT ZONE">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Special processing literal</Description>
							<ELFFieldNumber>0100</ELFFieldNumber>
						</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:simpleContent>
							<xsd:extension base="TextType">
								<xsd:attribute name="combatZoneDt" use="required">
									<xsd:annotation>
										<xsd:documentation>
											<ELFFieldNumber>0100</ELFFieldNumber>
										</xsd:documentation>
									</xsd:annotation>
									<xsd:simpleType>
										<xsd:restriction base="DateType">
											<xsd:pattern value="[1-9][0-9]{3}\-.*"/>
										</xsd:restriction>
									</xsd:simpleType>
								</xsd:attribute>
							</xsd:extension>
						</xsd:simpleContent>
					</xsd:complexType>
				</xsd:element>
				<!-- Special Processing Code Text -->
				<xsd:element name="SpecialProcessingCodeTxt">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Special processing code text</Description>
							<ELFFieldNumber>0100</ELFFieldNumber>
						</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="TextType">
							<xsd:maxLength value="22"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
			</xsd:choice>
			
			<!-- PECF Primary Indicator  -->
			<xsd:element name="PECFPrimaryInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>PECF primary indicator</Description>
						<ELFFieldNumber>0110</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- PECF Spouse Indicator -->
			<xsd:element name="PECFSpouseInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>PECF spouse indicator</Description>
						<ELFFieldNumber>0120</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- =============== Filing Status =============== -->
			<!-- Individual Return Filing Status Cd -->
			<xsd:element name="IndividualReturnFilingStatusCd">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Individual return filing status cd</Description>
						<ELFFieldNumber>0130</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="TextType">
						<xsd:enumeration value="1"/>
						<xsd:enumeration value="2"/>
						<xsd:enumeration value="3"/>
						<xsd:enumeration value="4"/>
						<xsd:enumeration value="5"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			
			<xsd:choice minOccurs="0">
				<!-- Spouses Name -->
				<xsd:element name="SpouseNm" type="PersonNameType" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Spouses name</Description>
							<ELFFieldNumber>0140</ELFFieldNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<!-- Non-Resident Alien literal code -->
				<xsd:element name="NRALiteralCd" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Non-Resident Alien literal code</Description>
							<ELFFieldNumber>0140</ELFFieldNumber>
						</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="TextType">
							<xsd:enumeration value="NRA"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>	
			</xsd:choice>

			<!-- Head of Household Qualifying Child Name -->
			<xsd:element name="QualifyingHOHNm" type="PersonNameType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Head of household qualifying child name</Description>
						<ELFFieldNumber>0150</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- Head of Household Qualifying Child SSN -->
			<xsd:element name="QualifyingHOHSSN" type="SSNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Head of household qualifying child SSN</Description>
						<ELFFieldNumber>0153</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- Qualifying Person Name -->
			<xsd:element name="QualifyingPersonName" type="PersonFullNameType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Qualifying person name</Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- Qualifying Person SSN -->
			<xsd:element name="QualifyingPersonSSN" type="SSNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Qualifying person SSN</Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- =============== Exemptions =============== -->
			<!-- Primary Claim As Dependent Indicator -->
			<xsd:element name="PrimaryClaimAsDependentInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Primary Claim As Dependent Indicator</Description>
						<ELFFieldNumber>0160</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- Spouse Claim As Dependent Indicator -->
			<xsd:element name="SpouseClaimAsDependentInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Spouse Claim As Dependent Indicator</Description>
						<ELFFieldNumber>0163</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<xsd:element name="ExemptSpouseNm" type="PersonNameType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Exempt spouse name</Description>
						<ELFFieldNumber>0164</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- Exempt Spouse Name Control -->
			<xsd:element name="ExemptSpouseNameControlTxt" type="PersonNameControlType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Exempt spouse name control</Description>
						<ELFFieldNumber>0165</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- Total Exempt Primary And Spouse Count -->
			<xsd:element name="TotalExemptPrimaryAndSpouseCnt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total exempt primary and spouse count</Description>
						<ELFFieldNumber>0167</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="IntegerNNType">
						<xsd:totalDigits value="1"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			
			<!-- Dependent Information Repeating Group -->
			<xsd:element name="DependentDetail" minOccurs="0" maxOccurs="100">
				<xsd:complexType>
					<xsd:sequence>
						<!-- Dependent First Name -->
						<xsd:element name="DependentFirstNm" type="PersonFirstNameType">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Dependent first name</Description>
									<LineNumber>(1)</LineNumber>
									<ELFFieldNumber>0170</ELFFieldNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<!-- Dependent Last Name -->
						<xsd:element name="DependentLastNm" type="PersonLastNameType">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Dependent last name</Description>
									<LineNumber>(1)</LineNumber>
									<ELFFieldNumber>0171</ELFFieldNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<!-- Dependent Name Control -->
						<xsd:element name="DependentNameControlTxt" type="PersonNameControlType">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Dependent name control</Description>
									<LineNumber>(1)</LineNumber>
									<ELFFieldNumber>0172</ELFFieldNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<!-- Dependent/Qualifying Child Identity Protection PIN -->
						<xsd:element name="IdentityProtectionPIN" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Dependent/Qualifying Child Identity Protection PIN</Description>
									<LineNumber>(2)</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="TextType">
									<xsd:length value="6"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<!-- Dependent SSN or "DiedLiteralCd" -->
						<xsd:choice>
							<xsd:element name="DependentSSN" type="SSNType">
								<xsd:annotation>
									<xsd:documentation>
										<Description>Dependent SSN</Description>
										<LineNumber>(2)</LineNumber>
										<ELFFieldNumber>0175</ELFFieldNumber>
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="DiedLiteralCd">
								<xsd:annotation>
									<xsd:documentation>
										<Description>Died Literal Code</Description>
										<LineNumber>(2)</LineNumber>
										<ELFFieldNumber>0175</ELFFieldNumber>
									</xsd:documentation>
								</xsd:annotation>
								<xsd:simpleType>
									<xsd:restriction base="TextType">
											<xsd:enumeration value="DIED"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
						</xsd:choice>
						<!-- Dependent Relationship -->
						<xsd:element name="DependentRelationshipCd">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Dependent relationship</Description>
									<LineNumber>(3)</LineNumber>
									<ELFFieldNumber>0177</ELFFieldNumber>
								</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="TextType">
									<xsd:enumeration value="SON"/>
									<xsd:enumeration value="DAUGHTER"/>
									<xsd:enumeration value="STEPCHILD"/>
									<xsd:enumeration value="FOSTER CHILD"/>
									<xsd:enumeration value="BROTHER"/>
									<xsd:enumeration value="SISTER"/>
									<xsd:enumeration value="STEPBROTHER"/>
									<xsd:enumeration value="STEPSISTER"/>
									<xsd:enumeration value="HALF BROTHER"/>
									<xsd:enumeration value="HALF SISTER"/>
									<xsd:enumeration value="GRANDCHILD"/>
									<xsd:enumeration value="NIECE"/>
									<xsd:enumeration value="NEPHEW"/>
									<xsd:enumeration value="PARENT"/>
									<xsd:enumeration value="GRANDPARENT"/>
									<xsd:enumeration value="AUNT"/>
									<xsd:enumeration value="UNCLE"/>
									<xsd:enumeration value="OTHER"/>
									<xsd:enumeration value="NONE"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<!-- Eligible for Child Tax Credit Indicator -->
					<xsd:choice minOccurs="0">
						<xsd:element name="EligibleForChildTaxCreditInd" type="CheckboxType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Eligible for child tax credit indicator</Description>
									<LineNumber>(4)</LineNumber>
									<ELFFieldNumber>0178</ELFFieldNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<!-- Eligible For Credit For Other Dependents Indicator -->
						<xsd:element name="EligibleForODCInd" type="CheckboxType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Eligible for credit for other dependents indicator</Description>
									<LineNumber>(4)</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
				       </xsd:choice>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			
			<!-- Number Of Other Dependents Listed Count Ind -->
			<xsd:element name="MoreDependentsInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>More than four dependents</Description>
						<ELFFieldNumber>0209</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- Number of Children Who Lived with You Count -->
			<xsd:element name="ChldWhoLivedWithYouCnt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Number of children who lived with you count</Description>
						<ELFFieldNumber>0240</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="IntegerNNType">
						<xsd:totalDigits value="2"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			
			<!-- Number of Children Not living With You Count-->
			<xsd:element name="ChldNotLivingWithYouCnt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Number of children not living with you count</Description>
						<ELFFieldNumber>0247</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="IntegerNNType">
						<xsd:totalDigits value="2"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			
			<!-- Number of Other Dependents Listed Count -->
			<xsd:element name="OtherDependentsListedCnt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Number of other dependents listed count</Description>
						<ELFFieldNumber>0350</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="IntegerNNType">
						<xsd:totalDigits value="2"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			
			<!-- Total Exemptions Count -->
			<xsd:element name="TotalExemptionsCnt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total exemptions count</Description>
						<ELFFieldNumber>0355</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="IntegerNNType">
						<xsd:totalDigits value="2"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			
			<!-- =============== Income =============== -->			
			<!-- Wages Salaries And Tips Amount -->
			<xsd:element name="WagesSalariesAndTipsAmt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Wages salaries and tips amount</Description>
						<LineNumber>1</LineNumber>
						<ELFFieldNumber>0375</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="USAmountType">
							<xsd:attribute name="wagesNotShownLitOnlyCd">
								<xsd:annotation>
									<xsd:documentation>
										<ELFFieldNumber>0370 0371</ELFFieldNumber>
									</xsd:documentation>
								</xsd:annotation>
								<xsd:simpleType>
									<xsd:restriction base="TextType">
										<xsd:enumeration value="FB"/>
										<xsd:enumeration value="DCB"/>
										<xsd:enumeration value="FB AND DCB"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:attribute>
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="IRSW2 NonW2DisabilityPaymentStatement WagesNotShownSchedule">
								<xsd:annotation>
									<xsd:documentation>
										<ELFFieldNumber>0356 0357 0358 0359 0360 0361 0362 0363 0366 0367 0368 0369 0370 0371 0372 0373 0378 0379</ELFFieldNumber>
									</xsd:documentation>
								</xsd:annotation>
							</xsd:attribute>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>
			
			<!-- Taxable Interest Amount -->
			<xsd:element name="TaxableInterestAmt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Taxable interest amount</Description>
						<LineNumber>2b</LineNumber>
						<ELFFieldNumber>0380</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="USAmountType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="IRS1040ScheduleB">
								<xsd:annotation>
									<xsd:documentation>
										<ELFFieldNumber>NL</ELFFieldNumber>
									</xsd:documentation>
								</xsd:annotation>
							</xsd:attribute>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>
			
			<!-- Tax-Exempt Interest Amount -->
			<xsd:element name="TaxExemptInterestAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Tax-exempt interest amount</Description>
						<LineNumber>2a</LineNumber>
						<ELFFieldNumber>0385</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- Ordinary F8814 Code -->
			<xsd:element name="OrdinaryF8814Cd" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Ordinary F8814 code</Description>
						<LineNumber>3b</LineNumber>
						<ELFFieldNumber>0390</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="TextType">
						<xsd:enumeration value="FORM 8814"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			
			<!-- Ordinary F8814 Amount -->
			<xsd:element name="OrdinaryF8814Amt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Ordinary F8814 amount</Description>
						<LineNumber>3b</LineNumber>
						<ELFFieldNumber>0391</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- Ordinary Dividends Amount -->
			<xsd:element name="OrdinaryDividendsAmt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Ordinary dividends amount</Description>
						<LineNumber>3b</LineNumber>
						<ELFFieldNumber>0394</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="USAmountType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="IRS1040ScheduleB">
								<xsd:annotation>
									<xsd:documentation>
										<ELFFieldNumber>NL</ELFFieldNumber>
									</xsd:documentation>
								</xsd:annotation>
							</xsd:attribute>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>
			
			<!-- Qualified F8814 Code -->
			<xsd:element name="QualifiedF8814Cd" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Qualified F8814 code</Description>
						<LineNumber>3a</LineNumber>
