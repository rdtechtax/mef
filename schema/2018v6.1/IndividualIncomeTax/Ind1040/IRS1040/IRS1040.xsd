							<Description>Taxpayer daytime telephone number</Description>
							<ELFFieldNumber>1328</ELFFieldNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<!-- Optional Foreign Telephone Number -->
				<xsd:element name="OptionalForeignTelephoneNum" type="ForeignPhoneNumberType" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Optional foreign telephone number</Description>
							<ELFFieldNumber>1329</ELFFieldNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>
			
