								<xsd:documentation>
									<Description>Credit for repayment code</Description>
									<LineNumber>Schedule 5, Line 74d</LineNumber>
									<ELFFieldNumber>1209</ELFFieldNumber>
								</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="TextType">
									<xsd:enumeration value="IRC 1341"/>
