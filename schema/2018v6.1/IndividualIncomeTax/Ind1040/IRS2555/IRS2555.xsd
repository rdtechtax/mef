<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.irs.gov/efile" targetNamespace="http://www.irs.gov/efile" elementFormDefault="qualified" attributeFormDefault="unqualified">

	<xsd:annotation>
		<xsd:documentation>
			<Description>IRS e-file Individual Income Tax Schema - IRS Form 2555 Foreign Earned Income</Description>
			<TaxYear>2018</TaxYear>
			<MaturityLevel>Final Release</MaturityLevel>
			<ReleaseDate>February 13, 2019</ReleaseDate>
		</xsd:documentation>
	</xsd:annotation>
	
	<xsd:include schemaLocation="../../../Common/efileTypes.xsd"/>
	
	<!-- ====================================================================== -->
	<!-- ============================ IRS Form 2555 =========================== -->
	<!-- ====================================================================== -->
	
	<xsd:element name="IRS2555">
		<xsd:annotation>
			<xsd:documentation>IRS Form 2555</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="IRS2555Type">
					<xsd:attributeGroup ref="DocumentAttributes">
						<xsd:annotation>
							<xsd:documentation>Common return document attributes</xsd:documentation>
						</xsd:annotation>
					</xsd:attributeGroup>
					<xsd:attribute name="documentName" type="xsd:string" fixed="IRS2555">
						<xsd:annotation>
							<xsd:documentation>IRS internal use only. To avoid error in the return do not include the attribute name or value.</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="claimFrgnEarnIncWaiverCd" type="TextType" fixed="CLAIMING WAIVER">
						<xsd:annotation>
							<xsd:documentation>
								<ELFNumber>0008 0009</ELFNumber>
							</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="referenceDocumentId" type="IdListType">
						<xsd:annotation>
							<xsd:documentation>List of document ID's of forms, schedules, supporting info. etc. attached to this form</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="referenceDocumentName" type="StringType" fixed="RevokeChoiceOfExclusionStatement ForeignEarnedIncomeWaiverOfTimeRequirementsStatement ExcludingIncomeInPriorYearStatement NoncashOrNonW2IncomeStatement">
						<xsd:annotation>
							<xsd:documentation>
								<ELFFieldNumber>NL NL NL NL</ELFFieldNumber>
							</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	
	<xsd:complexType name="IRS2555Type">
		<xsd:annotation>
			<xsd:documentation>Content model for Form 2555</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
		
			<!-- Name -->
			<xsd:element name="NameLine1Txt" type="NameLine1Type">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Name</Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- SSN  -->
			<xsd:element name="SSN" type="SSNType">
				<xsd:annotation>
					<xsd:documentation>
						<Description>SSN</Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- ===== Part I - General Information ==== -->

			<!-- Foreign Address -->
			<xsd:element name="ForeignAddress" type="ForeignAddressType">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Foreign Address</Description>
						<LineNumber>1</LineNumber>
						<ELFFieldNumber>0010 0011 0012 0013 0014 0015</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- Occupation -->
			<xsd:element name="OccupationTxt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Occupation</Description>
						<LineNumber>2</LineNumber>
						<ELFFieldNumber>0020</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="TextType">
						<xsd:maxLength value="35"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			
			<!-- Employer's Name -->
			<xsd:element name="EmployerName" type="BusinessNameType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Employer's Name</Description>
						<LineNumber>3</LineNumber>
						<ELFFieldNumber>0030 0040</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- Employer's US Address -->
			<xsd:element name="EmployerUSAddress" type="USAddressType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Employer's US Address</Description>
						<LineNumber>4a</LineNumber>
						<ELFFieldNumber>0041 0042 0043 0044</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- Employer's Foreign Address -->
			<xsd:element name="EmployerForeignAddress" type="ForeignAddressType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Employer's Foreign Address</Description>
						<LineNumber>4b</LineNumber>
						<ELFFieldNumber>0050 0051 0052 0053 0054 0055</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- Employer is a Foreign Entity Indicator -->
			<xsd:element name="EmployerForeignEntityInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Employer is a Foreign Entity Indicator</Description>
						<LineNumber>5a</LineNumber>
						<ELFFieldNumber>0060</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- Employer is a United States Company Indicator -->
			<xsd:element name="EmployerUnitedStatesCompanyInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Employer is a United States Company Indicator</Description>
						<LineNumber>5b</LineNumber>
						<ELFFieldNumber>0070</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- Self Employment Indicator -->
			<xsd:element name="SelfEmploymentInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Self Employment Indicator</Description>
						<LineNumber>5c</LineNumber>
						<ELFFieldNumber>0080</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- Employer is a Foreign Affiliate Employer Indicator -->
			<xsd:element name="ForeignAffiliateEmployerInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Employer is a Foreign Affiliate Employer Indicator</Description>
						<LineNumber>5d</LineNumber>
						<ELFFieldNumber>0090</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- Other Employer Type Group -->
			<xsd:element name="OtherEmployerTypeGroup" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>

						<!-- Other Employer Indicator -->
						<xsd:element name="OtherEmployerInd" type="CheckboxType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Other Employer Indicator</Description>
									<LineNumber>5e</LineNumber>
									<ELFFieldNumber>0100</ELFFieldNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>						

						<!-- Other Employer Description Text -->
						<xsd:element name="OtherEmployerDesc" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Other Employer Description Text</Description>
									<LineNumber>5e</LineNumber>
									<ELFFieldNumber>0105</ELFFieldNumber>
								</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="TextType">
									<xsd:maxLength value="35"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>

					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			
			<!-- Last Foreign Earned Income Exclusion Claimed Year -->
			<xsd:element name="LastFrgnEarnIncExclClaimedYr" type="YearType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Last Foreign Earned Income Exclusion Claimed Year</Description>
						<LineNumber>6a</LineNumber>
						<ELFFieldNumber>0110</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- No Foreign Earned Income Exclusion Previously Filed Indicator -->
			<xsd:element name="NoFrgnEarnIncExclPrevFiledInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>No Foreign Earned Income Exclusion Previously Filed Indicator</Description>
						<LineNumber>6b</LineNumber>
						<ELFFieldNumber>0120</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- Foreign Earned Income Exclusion Revoked Indicator -->
			<xsd:element name="ForeignEarnIncExclRevokedInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Foreign Earned Income Exclusion Revoked Indicator</Description>
						<LineNumber>6c</LineNumber>
						<ELFFieldNumber>0130 0140</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- Foreign Earned Income Exclusion Revoked Group for Line 6d -->
			<xsd:element name="ForeignEarnIncExclRevokeGrp" minOccurs="0" maxOccurs="10">
				<xsd:complexType>
					<xsd:sequence>
						<!-- Foreign Earned Income Exclusion Revoked Type Code -->
						<xsd:element name="ForeignEarnIncExclRevokeTypeCd" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Foreign Earned Income Exclusion Revoked Type Code</Description>
									<LineNumber>6d</LineNumber>
									<ELFFieldNumber>0150</ELFFieldNumber>
								</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="TextType">
									<xsd:enumeration value="FOREIGN EARNED INCOME EXCLUSION"/>
									<xsd:enumeration value="HOUSING EXCLUSION"/>
									<xsd:enumeration value="FOREIGN EARNED INCOME EXCLUSION AND HOUSING EXCLUSION"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>		
						<!-- Foreign Earned Income Exclusion Revoked Tax Year -->
						<xsd:element name="ForeignEarnIncExclRevokeTaxYr" type="YearType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Foreign Earned Income Exclusion Revoked Tax Year</Description>
									<LineNumber>6d</LineNumber>
									<ELFFieldNumber>0150</ELFFieldNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			
			<!-- Citizen Country Name -->
			<xsd:element name="CitizenCountryNm" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Citizen Country Name</Description>
						<LineNumber>7</LineNumber>
						<ELFFieldNumber>0160</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="TextType">
						<xsd:maxLength value="35"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			
			<!-- Separate Foreign Residence Indicator -->
			<xsd:element name="SeparateForeignResidenceInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Separate Foreign Residence Indicator</Description>
						<LineNumber>8a</LineNumber>
						<ELFFieldNumber>0170 0180</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- Separate Foreign Residence Location Text -->
			<xsd:element name="SeparateForeignResLocationTxt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Separate Foreign Residence Location Text</Description>
						<LineNumber>8b</LineNumber>
						<ELFFieldNumber>0190</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="TextType">
						<xsd:maxLength value="35"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			
			<!-- Separate Foreign Residence Day Count -->
			<xsd:element name="SeparateForeignResidenceDayCnt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Separate Foreign Residence Day Count</Description>
						<LineNumber>8b</LineNumber>
						<ELFFieldNumber>0200</ELFFieldNumber>
					</xsd:documentation> 
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="NumberOfDaysType">
						<xsd:totalDigits value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			
			<!-- Tax Home Description Text -->
			<xsd:element name="TaxHomeGroup" minOccurs="0" maxOccurs="50">
				<xsd:complexType>
					<xsd:sequence>

						<!-- Tax Home Description Text -->
						<xsd:element name="TaxHomeDesc" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Tax Home Description Text</Description>
									<LineNumber>9</LineNumber>
									<ELFFieldNumber>0210</ELFFieldNumber>
								</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="TextType">
									<xsd:maxLength value="35"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>						

						<!-- Established Date -->
						<xsd:element name="EstablishedDt" type="DateType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Established Date</Description>
									<LineNumber>9</LineNumber>
									<ELFFieldNumber>0215</ELFFieldNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			
			<!-- ===== Part II - Taxpayers Qualifying Under Bona Fide Residence Test ==== -->

			<xsd:element name="TaxpayerBonaFideResGrp" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>

						<!-- Bona Fide Residence Begin Date -->
						<xsd:element name="BonaFideResidenceBeginDt" type="DateType">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Bona Fide Residence Begin Date</Description>
									<LineNumber>10</LineNumber>
									<ELFFieldNumber>0220</ELFFieldNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						
						<xsd:choice>

							<!-- Bona Fide Residence End Date -->
							<xsd:element name="BonaFideResidenceEndDt" type="DateType">
								<xsd:annotation>
									<xsd:documentation>
										<Description>Bona Fide Residence End Date</Description>
										<LineNumber>10</LineNumber>
										<ELFFieldNumber>0225</ELFFieldNumber>
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>

							<!-- Bona Fide Residence Continued Code -->				
							<xsd:element name="BonaFideResidenceContinuedCd">
								<xsd:annotation>
									<xsd:documentation>
										<Description>Bona Fide Residence Continued Code</Description>
										<LineNumber>10</LineNumber>
										<ELFFieldNumber>0225</ELFFieldNumber>
									</xsd:documentation>
								</xsd:annotation>
								<xsd:simpleType>
									<xsd:restriction base="TextType">
										<xsd:enumeration value="CONTINUES"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>

						</xsd:choice>			
						
					</xsd:sequence>
				</xsd:complexType> 		
			</xsd:element>
			
			<xsd:choice minOccurs="0">

				<!-- Purchased House Indicator -->
				<xsd:element name="PurchasedHouseInd" type="CheckboxType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Purchased House Indicator</Description>
							<LineNumber>11a</LineNumber>
							<ELFFieldNumber>0230</ELFFieldNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>

				<!-- Rented House Indicator -->
				<xsd:element name="RentedHouseInd" type="CheckboxType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Rented House Indicator</Description>
							<LineNumber>11b</LineNumber>
							<ELFFieldNumber>0240</ELFFieldNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>

				<!-- Rented Room Indicator -->
				<xsd:element name="RentedRoomInd" type="CheckboxType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Rented Room Indicator</Description>
							<LineNumber>11c</LineNumber>
							<ELFFieldNumber>0250</ELFFieldNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>

				<!-- Employer Furnished Quarters Indicator -->
				<xsd:element name="EmployerFurnishedQuartersInd" type="CheckboxType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Employer Furnished Quarters Indicator</Description>
							<LineNumber>11d</LineNumber>
							<ELFFieldNumber>0260</ELFFieldNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>

			</xsd:choice>
			
			<!-- Family Lived Abroad Indicator -->
			<xsd:element name="FamilyLivedAbroadInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Family Lived Abroad Indicator</Description>
						<LineNumber>12a</LineNumber>
						<ELFFieldNumber>0270 0280</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<xsd:element name="FamilyLivingWithTaxpayerAbroad" minOccurs="0" maxOccurs="100">
				<xsd:complexType>
					<xsd:sequence>

						<!-- Family Living With You Time Period -->
						<xsd:element name="FamilyLivingWithYouTimePrdTxt" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Family Living With You Time Period</Description>
									<LineNumber>12b</LineNumber>
									<ELFFieldNumber>0295</ELFFieldNumber>
								</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="TextType">
									<xsd:maxLength value="25"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>				

						<!-- Family Relationship Code -->
						<xsd:element name="FamilyRelationshipCd" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Family Relationship Code</Description>
									<LineNumber>12b</LineNumber>
									<ELFFieldNumber>0290</ELFFieldNumber>
								</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="TextType">
									<xsd:enumeration value="CHILD"/>
									<xsd:enumeration value="FOSTERCHILD"/>
									<xsd:enumeration value="GRANDCHILD"/>
									<xsd:enumeration value="GRANDPARENT"/>
									<xsd:enumeration value="PARENT"/>
									<xsd:enumeration value="BROTHER"/>
									<xsd:enumeration value="SISTER"/>
									<xsd:enumeration value="AUNT"/>
									<xsd:enumeration value="UNCLE"/>
									<xsd:enumeration value="NEPHEW"/>
									<xsd:enumeration value="NIECE"/>
									<xsd:enumeration value="NONE"/>
									<xsd:enumeration value="SON"/>
									<xsd:enumeration value="DAUGHTER"/>
									<xsd:enumeration value="SPOUSE"/>
									<xsd:enumeration value="OTHER"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>

					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			
			<!-- Submitted Non-Resident Statement Indicator -->
			<xsd:element name="SubmittedNonResidentStmtInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Submitted Non-Resident Statement Indicator</Description>
						<LineNumber>13a</LineNumber>
						<ELFFieldNumber>0300 0310</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- Required to Pay Income Tax Indicator -->
			<xsd:element name="RequiredToPayIncomeTaxInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Required to Pay Income Tax Indicator</Description>
						<LineNumber>13b</LineNumber>
						<ELFFieldNumber>0320 0330</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- Presence in The US Group -->
			<xsd:element name="PresenceInTheUSGroup" minOccurs="0" maxOccurs="50">
				<xsd:complexType>
					<xsd:sequence>

						<!-- Arrived In US Date -->
						<xsd:element name="ArrivedInUSDt" type="DateType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Arrived In US Date</Description>
									<LineNumber>14a(1)</LineNumber>
									<ELFFieldNumber>0340 0348 0356 0372 0380 0388 0396 0404</ELFFieldNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Departed US Date -->
						<xsd:element name="DepartedUSDt" type="DateType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Departed US Date</Description>
									<LineNumber>14b(1)</LineNumber>
									<ELFFieldNumber>0342 0350 0358 0374 0382 0390 0398 0406</ELFFieldNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Business Days Count -->
						<xsd:element name="BusinessDaysCnt" type="NumberOfDaysType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Business Days Count</Description>
									<LineNumber>14c(1)</LineNumber>
									<ELFFieldNumber>0344 0352 0360 0376 0384 0392 0400 0408</ELFFieldNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Business Income Earned Amount -->
						<xsd:element name="BusinessIncomeEarnedAmt" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Business Income Earned Amount</Description>
									<LineNumber>14d(1)</LineNumber>
									<ELFFieldNumber>0346 0354 0370 0378 0386 0394 0402 0410 0415</ELFFieldNumber>
								</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:simpleContent>
									<xsd:extension base="USAmountType">
										<xsd:attribute name="referenceDocumentId" type="IdListType"/>
										<xsd:attribute name="referenceDocumentName" type="StringType" fixed="USEarnedIncomeComputationStatement"/>
									</xsd:extension>
								</xsd:simpleContent>
							</xsd:complexType>
						</xsd:element>

					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			
			<!-- Employment Contract Terms Description -->
			<xsd:element name="EmploymentContractTermsDesc" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Employment Contract Terms Description</Description>
						<LineNumber>15a</LineNumber>
						<ELFFieldNumber>0420</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="TextType">
						<xsd:maxLength value="80"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			
			<!-- Visa Type Description -->
			<xsd:element name="VisaTypeDesc" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Visa Type Description</Description>
						<LineNumber>15b</LineNumber>
						<ELFFieldNumber>0430</ELFFieldNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="TextType">
						<xsd:maxLength value="30"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			
			<!-- Visa Limit Stay or Employment Indicator -->
			<xsd:element name="VisaLimitStayOrEmploymentInd" minOccurs="0">
				<xsd:annotation>
