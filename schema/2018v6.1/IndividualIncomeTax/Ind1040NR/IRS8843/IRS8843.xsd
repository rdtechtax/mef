						</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="TextType">
							<xsd:enumeration value="INFORMATION PROVIDED ON FORM 1040NR"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
