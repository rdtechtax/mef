														<xsd:documentation>
															<Description>Visa change description</Description>
															<LineNumber>11</LineNumber>
														</xsd:documentation>
													</xsd:annotation>
													<xsd:simpleType>
														<xsd:restriction base="TextType">
															<xsd:maxLength value="30"/>
